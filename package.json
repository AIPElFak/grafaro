{
  "name": "grafaro",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/lazarljubenovic/grafaro"
  },
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "test-single": "ng test --single-run",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "ts:build": "tsc -p ./server",
    "ts:watch": "tsc -p ./server -w",
    "serve": "nodemon ./server/index.js",
    "docs": "rm -rf ./docs/models/ && typedoc --module commonjs --mode modules --moduleResolution node --preserveConstEnums --target es6 --includeDeclarations --emitDecoratorMetadata --experimentalDecorators --out ./docs/models ./src/app/models/graph.model.ts ./src/app/models/id-generator.ts ./src/app/data-structures/queue.ts ./src/app/data-structures/stack.ts --ignoreCompilerErrors --exclude node_modules --externalPattern node_modules --excludeExternals"
  },
  "pre-commit": [
    "lint",
    "test-single"
  ],
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/forms": "^2.4.4",
    "@angular/http": "^2.4.4",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "@angular/router": "^3.4.4",
    "@angular/upgrade": "^2.4.4",
    "@lazarljubenovic/vis-ng": "^0.1.13",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "@ngtools/webpack": "^1.2.4",
    "angular2-jwt": "^0.1.28",
    "auth0-lock": "^10.10.2",
    "body-parser": "^1.15.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "deep-assign": "^2.0.0",
    "esprima": "^3.1.3",
    "express": "^4.14.0",
    "graphlib": "^2.1.1",
    "jdenticon": "^1.3.2",
    "marked": "^0.3.6",
    "mongoose": "^4.7.4",
    "nested-property": "^0.0.7",
    "rollup": "^0.41.4",
    "rxjs": "^5.0.3",
    "sha1": "^1.1.1",
    "ts-helpers": "^1.1.1",
    "typedoc": "^0.5.7",
    "vis": "^4.16.1",
    "ws": "^1.1.1",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@types/auth0-lock": "^10.0.21",
    "@types/body-parser": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/deep-assign": "^0.1.0",
    "@types/esprima": "^2.1.33",
    "@types/express": "^4.0.34",
    "@types/jasmine": "^2.2.30",
    "@types/marked": "^0.0.28",
    "@types/mongoose": "^4.7.2",
    "@types/node": "^6.0.42",
    "@types/ws": "0.0.37",
    "angular-cli": "^1.0.0-beta.28.3",
    "codelyzer": "~0.0.26",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.1.3",
    "protractor": "4.0.9",
    "sass-material-colors": "^0.0.5",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2"
